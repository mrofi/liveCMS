{
    "name": "livecms/core",
    "description": "CMS package for laravel 8.x or newest",
    "keywords": ["package", "laravel", "cms", "livecms"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/livecms/core/issues",
        "source": "https://github.com/livecms/core"
    },
    "authors": [
        {
            "name": "Mokhamad Rofiudin",
            "email": "mokh.rofiudin@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Livecms\\Cms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
